@article{Chen2019,
	title        = {{The association between high ambient air pollution exposure and respiratory health of young children: A cross sectional study in Jinan, China}},
	author       = {Chen, Zhangjian and Cui, Liangliang and Cui, Xiaoxing and Li, Xinwei and Yu, Kunkun and Yue, Kesan and Dai, Zhixiang and Zhou, Jingwen and Jia, Guang and Zhang, Ji},
	year         = 2019,
	journal      = {Science of the Total Environment},
	publisher    = {Elsevier B.V.},
	volume       = 656,
	pages        = {740--749},
	doi          = {10.1016/j.scitotenv.2018.11.368},
	issn         = 18791026,
	url          = {https://doi.org/10.1016/j.scitotenv.2018.11.368},
	abstract     = {There is growing concern about health effects of high air pollution in less-developed countries. Children represent a population at increased risk for air pollutionârelated respiratory conditions. This study investigated the relationship between high ambient air pollution exposure and respiratory health of young schoolchildren. From 2014 to 2016 in Jinan, China, 2532 primary school children in grades three to five from two different schools with different levels of air pollution were included in the study. Levels of ambient air pollution exposure including PM10, PM2.5, NO2, SO2, CO and O3 were measured continuously at the two schools. A questionnaire about children's respiratory health was conducted every year. Among them, about 150 randomly selected children also performed lung function tests two times a year at the beginning of November and middle of December. Annual average exposure levels of PM2.5 (66.8â79.1 vs 90.0â107.7 $\mu$g/m3), PM10 (129.5â177.3 vs 198.1â218.6 $\mu$g/m3), NO2 (45.3â53.2 vs 45.0â56.2 $\mu$g/m3), SO2 (29.8â56.5 vs 40.5â80.3 $\mu$g/m3), CO (1.3â1.5 vs 1.4â1.7 mg/m3) and O3 (84.8â120.2 vs 61.1â128.1 $\mu$g/m3) in the heavy pollution primary school were significantly higher than the light one. The higher air pollution exposure was related to increased prevalence of respiratory diseases of young children in the last year, especially allergic rhinitis. The increased odds of lung function impairment associated with exposure to higher air pollution, could be up to 171.5% (aOR = 2.715; 95% CI = 1.915â3.849) for PEF < 75% predicted in 2014. However, after short-term exposure for 1.5 month or a week, paired comparison for parameters of the same child showed different results. The association between high ambient air pollution exposure and respiratory health of young children is closely related to exposure time and dose and may be fluctuate and complex.},
	file         = {:Users/davidlary/Library/Application Support/Mendeley Desktop/Downloaded/Chen et al. - 2019 - The association between high ambient air pollution exposure and respiratory health of young children A cross sectio.pdf:pdf},
	keywords     = {Ambient air pollution,Lung function,Respiratory health,Young children},
	pmid         = 30530144
}
