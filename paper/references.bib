@article{Mordecai2017,
	title        = {{Detecting the impact of temperature on transmission of Zika, dengue, and chikungunya using mechanistic models}},
	author       = {Mordecai, Erin A. and Cohen, Jeremy M. and Evans, Michelle V. and Gudapati, Prithvi and Johnson, Leah R. and Lippi, Catherine A. and Miazgowicz, Kerri and Murdock, Courtney C. and Rohr, Jason R. and Ryan, Sadie J. and Savage, Van and Shocket, Marta S. and {Stewart Ibarra}, Anna and Thomas, Matthew B. and Weikel, Daniel P.},
	year         = 2017,
	journal      = {PLoS Neglected Tropical Diseases},
	volume       = 11,
	number       = 4,
	pages        = {1--18},
	doi          = {10.1371/journal.pntd.0005568},
	isbn         = 2009351020543,
	issn         = 19352735,
	abstract     = {Recent epidemics of Zika, dengue, and chikungunya have heightened the need to understand the seasonal and geographic range of transmission by Aedes aegypti and Ae. albopictus mosquitoes. We use mechanistic transmission models to derive predictions for how the probability and magnitude of transmission for Zika, chikungunya, and dengue change with mean temperature, and we show that these predictions are well matched by human case data. Across all three viruses, models and human case data both show that transmission occurs between 18â34Â°C with maximal transmission occurring in a range from 26â29Â°C. Controlling for population size and two socioeconomic factors, temperature-dependent transmission based on our mechanistic model is an important predictor of human transmission occurrence and incidence. Risk maps indicate that tropical and subtropical regions are suitable for extended seasonal or year-round transmission, but transmission in temperate areas is limited to at most three months per year even if vectors are present. Such brief transmission windows limit the likelihood of major epidemics following disease introduction in temperate zones.},
	file         = {:Users/davidlary/Library/Application Support/Mendeley Desktop/Downloaded/Mordecai et al. - 2017 - Detecting the impact of temperature on transmission of Zika, dengue, and chikungunya using mechanistic models.pdf:pdf},
	pmid         = 28448507
}
