@article{Gonzalez-Pleiter2021,
	title        = {{Occurrence and transport of microplastics sampled within and above the planetary boundary layer}},
	author       = {Gonz{\'{a}}lez-Pleiter, Miguel and Edo, Carlos and Aguilera, {\'{A}}ngeles and Vi{\'{u}}dez-Moreiras, Daniel and Pulido-Reyes, Gerardo and Gonz{\'{a}}lez-Toril, Elena and Osuna, Susana and de Diego-Castilla, Graciela and Legan{\'{e}}s, Francisco and Fern{\'{a}}ndez-Pi{\~{n}}as, Francisca and Rosal, Roberto},
	year         = 2021,
	journal      = {Science of the Total Environment},
	publisher    = {Elsevier B.V.},
	volume       = 761,
	pages        = 143213,
	doi          = {10.1016/j.scitotenv.2020.143213},
	issn         = 18791026,
	url          = {https://doi.org/10.1016/j.scitotenv.2020.143213},
	abstract     = {Nowadays, there is no direct evidence about the presence of microplastics (MPs) in the atmosphere above ground level. Here, we investigated the occurrence, chemical composition, shape, and size of MPs in aircraft sampling campaigns flying within and above the planetary boundary layer (PBL). The results showed that MPs were present with concentrations ranging from 1.5 MPs mâ3 above rural areas to 13.9 MPs mâ3 above urban areas. MPs represented up to almost one third of the total amount of microparticles collected. Fourier Transform Infrared Spectroscopy allowed identifying seven types of MPs with the highest diversity corresponding to urban areas. Atmospheric transport and deposition simulations were performed using the HYbrid Single-Particle Lagrangian Integrated Trajectory (HYSPLIT) model. Air mass trajectory analyses showed that MPs could be transported more than 1000 km before being deposited. This pioneer study is the first evidence of the microplastic presence above PBL and their potential long-range transport from their point of release even crossing distant borders.},
	file         = {:Users/davidlary/Library/Application Support/Mendeley Desktop/Downloaded/Gonz{\'{a}}lez-Pleiter et al. - 2021 - Occurrence and transport of microplastics sampled within and above the planetary boundary layer.pdf:pdf},
	keywords     = {Airborne microplastics,Aircraft sampling,Atmospheric transport,Microplastics deposition,Planetary boundary layer},
	pmid         = 33162145
}
